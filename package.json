{
    "name": "@meigyokuthmn/iconv-lite",
    "description": "Convert character encodings in pure javascript.",
    "version": "0.7.0-pre",
    "license": "MIT",
    "keywords": [
        "iconv",
        "convert",
        "charset",
        "icu"
    ],
    "author": "Meigyoku Thmn",
    "main": "./lib/index-node.js",
    "typings": "./lib/index.d.ts",
    "homepage": "https://github.com/Meigyoku-Thmn/iconv-lite",
    "bugs": "https://github.com/Meigyoku-Thmn/iconv-lite/issues",
    "repository": {
        "type": "git",
        "url": "git://github.com/Meigyoku-Thmn/iconv-lite.git"
    },
    "engines": {
        "node": ">=4.5.0 <5.0.0 || >=5.10.0"
    },
    "scripts": {
        "coverage": "c8 _mocha --grep .",
        "format": "prettier --write .",
        "lint": "eslint --max-warnings 0 . && prettier --check .",
        "test-install": "mv package.json package-orig.json && jq '.devDependencies |= {mocha, iconv, unorm}' package-orig.json > package.json && npm install; mv -f package-orig.json package.json",
        "test": "mocha --grep .",
        "test-node-web": "ICONV_BACKEND=web mocha --grep '#node-web'"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged --concurrent=false"
        }
    },
    "browser": {
        "./lib/index-node.js": "./lib/index-web.js",
        "stream": false
    },
    "devDependencies": {
        "c8": "^7.2.0",
        "eslint": "^7.4.0",
        "eslint-plugin-es": "^3.0.1",
        "husky": "^4.2.5",
        "iconv": "^2.3.5",
        "lint-staged": "^10.2.11",
        "mocha": "^3.5.3",
        "prettier": "2.0.5",
        "unorm": "^1.6.0"
    },
    "dependencies": {
        "safer-buffer": ">= 2.1.2 < 3.0.0"
    }
}
